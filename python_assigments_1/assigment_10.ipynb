      "source": [
        "def carpanlarina_ayir(sayi):\n",
        "  carpanlar=[]\n",
        "  for i in range(1,sayi+1):\n",
        "    if sayi % i ==0:\n",
        "      carpanlar.append(i)\n",
        "      \n",
        "  return carpanlar\n",
        "\n",
        "\n",
        "print(carpanlarina_ayir(250))"
      ],
