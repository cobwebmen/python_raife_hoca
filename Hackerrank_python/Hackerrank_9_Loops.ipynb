        "id": "RbSnDFrxchGN"
      },
      "source": [
        "if __name__ == '__main__':\n",
        "    n = int(input())\n",
        "    i=0\n",
        "    while i<n:\n",
        "        print(i**2)\n",
        "        i+=1"
