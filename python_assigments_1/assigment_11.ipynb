        "def factorial(sayi):\n",
        "  sonuc=1\n",
        "  i=1 \n",
        "  while i<sayi+1:\n",
        "    sonuc*=i\n",
        "    i+=1\n",
        "  return sonuc\n",
        "\n",
        "print(factorial(8))"
